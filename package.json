{
  "name": "@livelybone/mouse-wheel",
  "version": "4.0.0",
  "description": "A module for bind mouse-wheel event. typescript supported",
  "main": "./lib/umd/index.js",
  "module": "./lib/es/index.js",
  "unpkg": "./lib/umd/index.js",
  "types": "./index.d.ts",
  "scripts": {
    "build": "rimraf ./lib && cross-env NODE_ENV=production rollup -c rollup.config.js",
    "build:test": "rimraf ./test-lib && cross-env NODE_ENV=test rollup -c rollup.config.test.js --sourcemap",
    "eslint": "eslint ./ --ext .ts,.js --fix",
    "test": "npm run build:test && cross-env NODE_ENV=test istanbul cover node_modules/mocha/bin/_mocha -- ./test",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "commit": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/livelybone/mouse-wheel.git"
  },
  "keywords": [
    "mousewheel",
    "wheel",
    "firefox",
    "chrome",
    "IE",
    "same delta"
  ],
  "author": "2631541504@qq.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/livelybone/mouse-wheel/issues"
  },
  "homepage": "https://github.com/livelybone/mouse-wheel#readme",
  "devDependencies": {
    "@babel/core": "^7.5.0",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/preset-env": "^7.5.0",
    "@babel/runtime": "^7.5.1",
    "@typescript-eslint/parser": "^1.11.0",
    "babel-plugin-istanbul": "^5.1.4",
    "chai": "^4.2.0",
    "commitizen": "^3.0.7",
    "conventional-changelog-cli": "^2.0.12",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-typescript": "^0.14.0",
    "husky": "^3.0.0",
    "istanbul": "^1.1.0-alpha.1",
    "lint-staged": "^9.1.0",
    "mocha": "^5.2.0",
    "prettier": "^1.18.2",
    "rollup": "^1.7.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-dts": "^1.1.5",
    "rollup-plugin-license": "^0.8.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-typescript2": "^0.21.2",
    "rollup-plugin-uglify": "^6.0.2",
    "typescript": "^3.5.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,ts}": [
      "eslint --fix",
      "git update-index --again"
    ],
    "**/*.scss": [
      "prettier --write",
      "git update-index --again"
    ]
  }
}
